				<div class="page-content">
					<div class="page-header">
						<h1>个人信息查询</h1>
					</div>
					<div class="row">
						<div class="col-xs-12">
							<div class="row">
								<div class="col-xs-12">
									<div class="table-header">
										<font style="vertical-align: inherit;">用户详情</font>
									</div>
									<table id="sample-table-2" class="table table-striped table-bordered table-hover role-list">
										<thead>
											<tr>
												<th><i class="fa fa-user bigger-110 hidden-480" aria-hidden="true"></i>用户名</th>
												<th><i class="fa fa-user-circle-o bigger-110 hidden-480" aria-hidden="true"></i>手机号码</th>
												<th><i class="fa fa-users bigger-110 hidden-480" aria-hidden="true"></i>邮箱</th>
												<th><i class="fa fa-clock-o bigger-110 hidden-480" aria-hidden="true"></i>注册时间</th>
												<th><i class="fa fa-cog fa-spin bigger-110 hidden-480" aria-hidden="true"></i>操作</th>
											</tr>
										</thead>
										<tbody>
										</tbody>
									</table>
								</div>
							</div>
						</div>
						<!--修改用户信息-->
						<div class="modal fade" id="user-form-div" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
							<div class="modal-dialog modal-md" role="document">
								<div class="modal-content">
									<div class="modal-header">
										<button type="button" class="close" data-dismiss="modal">&times;</button>
										<h4 class="blue bigger user-form-title">修改个人信息</h4>
									</div>
									<form id="user-form" name="user-form" class="user-form">
										<div class="modal-body">
											<div class="row">
												<div class="col-xs-12 col-sm-6">
													<input type="hidden" name="id" class="form-control">
													<div class="form-group">
														<label for="form-add-username">用户名<span
                                                                style="color:#F00">*</span></label>
														<div>
															<input type="text" id="form-add-username" name="form-add-username" placeholder="用户名" />
														</div>
														<label for="form-add-emain">邮箱<span
                                                                style="color:#F00">*</span></label>
														<div>
															<input type="text" id="form-add-email" name="form-add-emain" placeholder="邮箱" />
														</div>
													</div>
												</div>
												<div class="col-xs-12 col-sm-6">
													<div class="form-group">

														<label for="form-add-tel">手机号码<span
                                                                style="color:#F00">*</span></label>
														<div>
															<input type="tel" id="form-add-tel" name="form-add-tel" placeholder="手机号码" />
														</div>
													</div>

												</div>
											</div>
										</div>
									</form>
									<div class="modal-footer">
										<button class="btn btn-sm" data-dismiss="modal" type="submit"><i
                                                class="ace-icon fa fa-times"></i>取消
                                        </button>
										<button type="button" class="btn btn-sm btn-primary role-submit"><i class="ace-icon fa fa-check"></i>确定</button>
									</div>
								</div>

							</div>
						</div>
						<!-- 提示框 -->
						<div class="modal fade" id="op-tips-dialog" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
							<div class="modal-dialog modal-sm" role="document">
